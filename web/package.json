{
  "name": "trxy",
  "version": "0.1.0",
  "description": "View Visual Studio Test results files (`.trx`) on any platform without the need for Visual Studio.",
  "private": true,
  "homepage": "./",
  "author": {
    "name": "mtusk",
    "email": "suport@mtusk.com",
    "url": "https://github.com/mtusk"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "npm run stage",
    "prestage": "rimraf ../app/wwwroot/ && mkdir ../app/wwwroot",
    "stage": "cp -r ./build/* ./build/**/* ../app/wwwroot/",
    "sample-trx": "dotnet test examples/TrxySampleTests/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.8.1",
    "@material-ui/icons": "^4.5.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "material-table": "^1.54.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "typeface-roboto": "0.0.75",
    "typescript": "~3.7.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/xml2js": "^0.4.5",
    "copyfiles": "^2.4.1",
    "cpy-cli": "^3.1.1",
    "rimraf": "^3.0.2",
    "wait-on": "^3.3.0"
  }
}
